{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "arrow-parens": "off",
        "object-curly-spacing": "off",
        "indent": "off",
        "linebreak-style": "off",
        "max-len": "off"
    }
}